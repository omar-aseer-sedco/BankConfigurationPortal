@model BankConfigurationPortal.Web.Models.Branch

<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">@Html.DisplayFor(model => model.NameEn)</h3>
    </div>
    <div class="panel-body">
        @Html.ActionLink(WebResources.Edit, "Edit", new { branchId = Model.BranchId }) | 
        @Html.ActionLink(WebResources.Details, "Details", new { branchId = Model.BranchId }) | 
        @Html.ActionLink(WebResources.Delete, "Delete", new { branchId = Model.BranchId })
    </div>
</div>
